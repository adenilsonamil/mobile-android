<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:background="@drawable/background">

	<ImageView android:layout_x="0sp" android:layout_y="0sp"
		android:id="@+id/ImageView01" android:layout_width="wrap_content"
		android:layout_height="40sp"/>

	<Button android:id="@+id/Button_Close" android:layout_x="02sp"
		android:layout_y="02sp" android:layout_width="wrap_content"
		android:layout_height="38sp" />

	<Button android:id="@+id/Button_Language_Help"
		android:layout_width="30sp" android:layout_height="30sp"
		android:layout_x="285sp" android:layout_y="05sp" android:background="@drawable/setting1" />

	<TextView android:id="@+id/TextView_TitleBar"
		android:layout_x="0sp" android:layout_y="40sp" android:layout_width="320sp"
		android:layout_height="40sp" android:textSize="18sp" />
	<ListView android:id="@+id/ListView_Gadgets" android:layout_x="0sp"
		android:layout_y="80sp" android:layout_width="320sp"
		android:layout_height="380sp" />

</AbsoluteLayout>
